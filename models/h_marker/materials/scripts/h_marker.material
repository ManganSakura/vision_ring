material HMarker/Diffuse
{
  receive_shadows on
  technique
  {
    pass
    {
      lighting on
      # 开启透明混合开关
      scene_blend alpha_blend
      depth_check on

      texture_unit
      {
        texture A_H_transparent.png
        filtering anisotropic
        max_anisotropy 16
      }
    }
  }
}
